{
  "resourceType": "StructureDefinition",
  "kind": "resource",
  "type": "Observation",
  "name": "MyObservation",
  "id": "my-profile",
  "differential": {
    "element": [
      {
        "id": "Observation.valueString",
        "path": "Observation.valueString",
        "min": 1,
        "patternString": "foo",
        "short": "bar"
      },
      {
        "id": "Observation.note",
        "path": "Observation.note",
        "constraint": [
          {
            "key": "myo-1",
            "severity": "warning",
            "human": "SHALL include the weather."
          },
          {
            "key": "myo-2",
            "severity": "error",
            "human": "SHALL include whether there is weather."
          }
        ]
      }
    ]
  }
}
